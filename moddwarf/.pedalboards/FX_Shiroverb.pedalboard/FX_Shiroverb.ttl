<xfade>
    ingen:canvasX 767.0 ;
    ingen:canvasY 400.0 ;
    ingen:enabled false ;
    ingen:polyphonic false ;
    lv2:microVersion 0 ;
    lv2:minorVersion 516 ;
    mod:builderVersion 1 ;
    mod:releaseNumber 6 ;
    lv2:port <xfade/inA_left> ,
             <xfade/inA_right> ,
             <xfade/inB_left> ,
             <xfade/inB_right> ,
             <xfade/out_left> ,
             <xfade/out_right> ,
             <xfade/xfade> ,
             <xfade/shape> ,
             <xfade/mode> ,
             <xfade/:bypass> ;
    lv2:prototype <http://gareus.org/oss/lv2/xfade> ;
    pedal:instanceNumber 3 ;
    pedal:preset <> ;
    a ingen:Block .

<xfade/inA_left>
    a lv2:AudioPort ,
        lv2:InputPort .

<xfade/inA_right>
    a lv2:AudioPort ,
        lv2:InputPort .

<xfade/inB_left>
    a lv2:AudioPort ,
        lv2:InputPort .

<xfade/inB_right>
    a lv2:AudioPort ,
        lv2:InputPort .

<xfade/inA_left>
    a lv2:AudioPort ,
        lv2:OutputPort .

<xfade/inA_right>
    a lv2:AudioPort ,
        lv2:OutputPort .

<xfade/inB_left>
    a lv2:AudioPort ,
        lv2:OutputPort .

<xfade/inB_right>
    a lv2:AudioPort ,
        lv2:OutputPort .

<xfade/shape>
    ingen:value 0.000000 ;
