<xfade>
    ingen:canvasX 777.0 ;
    ingen:canvasY 288.0 ;
    ingen:enabled true ;
    ingen:polyphonic false ;
    lv2:microVersion 0 ;
    lv2:minorVersion 516 ;
    mod:builderVersion 1 ;
    mod:releaseNumber 6 ;
    lv2:port <xfade/inA_left> ,
             <xfade/inA_right> ,
             <xfade/inB_left> ,
             <xfade/inB_right> ,
             <xfade/out_left> ,
             <xfade/out_right> ,
             <xfade/xfade> ,
             <xfade/shape> ,
             <xfade/mode> ,
             <xfade/:bypass> ;
    lv2:prototype <http://gareus.org/oss/lv2/xfade> ;
    pedal:instanceNumber 3 ;
    pedal:preset <> ;
    a ingen:Block .

<xfade/inA_left>
    a lv2:AudioPort ,
        lv2:InputPort .

<xfade/inA_right>
    a lv2:AudioPort ,
        lv2:InputPort .

<xfade/inB_left>
    a lv2:AudioPort ,
        lv2:InputPort .

<xfade/inB_right>
    a lv2:AudioPort ,
        lv2:InputPort .

<xfade/inA_left>
    a lv2:AudioPort ,
        lv2:OutputPort .

<xfade/inA_right>
    a lv2:AudioPort ,
        lv2:OutputPort .

<xfade/inB_left>
    a lv2:AudioPort ,
        lv2:OutputPort .

<xfade/inB_right>
    a lv2:AudioPort ,
        lv2:OutputPort .

<xfade/shape>
    ingen:value 0.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<xfade/xfade>
    ingen:value 1.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<xfade/mode>
    ingen:value 0.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<xfade/:bypass>
    ingen:value 0 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Granulator>
    ingen:canvasX 1876.0 ;
    ingen:canvasY 306.0 ;
    ingen:enabled true ;
    ingen:polyphonic false ;
    lv2:microVersion 0 ;
    lv2:minorVersion 1 ;
    mod:builderVersion 1 ;
    mod:releaseNumber 5 ;
    lv2:port <Granulator/in0> ,
             <Granulator/in1> ,
             <Granulator/out0> ,
             <Granulator/out1> ,
             <Granulator/delay_length> ,
             <Granulator/grain_density> ,
             <Granulator/grain_length> ,
             <Granulator/mix> ,
             <Granulator/:bypass> ;
    lv2:prototype <http://faust-lv2.googlecode.com/Granulator> ;
    pedal:instanceNumber 4 ;
    pedal:preset <> ;
    a ingen:Block .

<Granulator/in0>
    a lv2:AudioPort ,
        lv2:InputPort .

<Granulator/in1>
    a lv2:AudioPort ,
        lv2:InputPort .

<Granulator/in0>
    a lv2:AudioPort ,
        lv2:OutputPort .

<Granulator/in1>
    a lv2:AudioPort ,
        lv2:OutputPort .

<Granulator/mix>
    ingen:value 0.500000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Granulator/grain_length>
    ingen:value 0.100000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Granulator/delay_length>
    ingen:value 10.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Granulator/grain_density>
    ingen:value 2.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Granulator/:bypass>
    ingen:value 0 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Gain2x2>
