        lv2:InputPort .

<Gain2x2/:bypass>
    ingen:value 0 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<:bpb>
    ingen:value 4.000000 ;
    lv2:index 0 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<:bpm>
    ingen:value 120.000000 ;
    lv2:index 1 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<:rolling>
