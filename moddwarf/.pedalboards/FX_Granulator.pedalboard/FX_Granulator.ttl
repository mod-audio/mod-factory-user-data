<Gain2x2/In1>
    a lv2:AudioPort ,
        lv2:InputPort .

<Gain2x2/In2>
    a lv2:AudioPort ,
        lv2:InputPort .

<Gain2x2/In1>
    a lv2:AudioPort ,
        lv2:OutputPort .

<Gain2x2/In2>
    a lv2:AudioPort ,
        lv2:OutputPort .

<Gain2x2/Gain>
