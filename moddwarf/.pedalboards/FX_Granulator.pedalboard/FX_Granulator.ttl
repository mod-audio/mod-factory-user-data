    ingen:canvasY 308.1 ;
    ingen:enabled true ;
    ingen:polyphonic false ;
    lv2:microVersion 0 ;
    lv2:minorVersion 1 ;
    mod:builderVersion 1 ;
    mod:releaseNumber 16 ;
