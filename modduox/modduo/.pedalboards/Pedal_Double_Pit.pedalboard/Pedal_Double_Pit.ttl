<HardwareBypass>
    ingen:canvasX 2225.0 ;
    ingen:canvasY 720.0 ;
    ingen:enabled false ;
    ingen:polyphonic false ;
    lv2:microVersion 0 ;
    lv2:minorVersion 1 ;
    mod:builderVersion 1 ;
    mod:releaseNumber 5 ;
    lv2:port <HardwareBypass/enabled> ,
             <HardwareBypass/mode> ,
             <HardwareBypass/:bypass> ;
    lv2:prototype <http://moddevices.com/plugins/mod-devel/HardwareBypass> ;
    pedal:preset <> ;
    a ingen:Block .

<HardwareBypass/enabled>
    ingen:value 0.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<HardwareBypass/mode>
    ingen:value 0.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<HardwareBypass/:bypass>
    ingen:value 1 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<SwitchBox2>
    ingen:canvasX 497.0 ;
    ingen:canvasY 224.0 ;
    ingen:enabled true ;
    ingen:polyphonic false ;
    lv2:microVersion 1 ;
    lv2:minorVersion 1 ;
    mod:builderVersion 1 ;
    mod:releaseNumber 10 ;
    lv2:port <SwitchBox2/In> ,
             <SwitchBox2/Out1> ,
             <SwitchBox2/Out2> ,
             <SwitchBox2/Switch> ,
             <SwitchBox2/:bypass> ;
    lv2:prototype <http://moddevices.com/plugins/mod-devel/SwitchBox2> ;
    pedal:preset <> ;
    a ingen:Block .

<SwitchBox2/In>
    a lv2:AudioPort ,
        lv2:InputPort .

<SwitchBox2/In>
    a lv2:AudioPort ,
        lv2:OutputPort .

<SwitchBox2/Switch>
    ingen:value 1.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<SwitchBox2/:bypass>
    ingen:value 0 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<_2Voices>
    ingen:canvasX 1191.0 ;
    ingen:canvasY 496.0 ;
    ingen:enabled true ;
    ingen:polyphonic false ;
    lv2:microVersion 4 ;
    lv2:minorVersion 0 ;
    mod:builderVersion 1 ;
    mod:releaseNumber 11 ;
    lv2:port <_2Voices/In> ,
             <_2Voices/Out1> ,
             <_2Voices/Out2> ,
             <_2Voices/Step1> ,
             <_2Voices/Step2> ,
             <_2Voices/Gain_1> ,
             <_2Voices/Gain_2> ,
             <_2Voices/Fidelity> ,
             <_2Voices/:bypass> ;
    lv2:prototype <http://moddevices.com/plugins/mod-devel/2Voices> ;
    pedal:preset <> ;
    a ingen:Block .

<_2Voices/In>
    a lv2:AudioPort ,
        lv2:InputPort .

<_2Voices/In>
    a lv2:AudioPort ,
        lv2:OutputPort .

<_2Voices/Step2>
    ingen:value -12.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<_2Voices/Fidelity>
    ingen:value 3.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<_2Voices/Step1>
    ingen:value 7.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<_2Voices/Gain_2>
    ingen:value 13.750000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<_2Voices/Gain_1>
    ingen:value 0.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<_2Voices/:bypass>
    ingen:value 0 ;
    a lv2:ControlPort ,
        lv2:InputPort .

