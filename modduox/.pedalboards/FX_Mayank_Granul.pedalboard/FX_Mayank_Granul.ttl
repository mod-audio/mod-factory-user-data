<Granulator>
    ingen:canvasX 1731.0 ;
    ingen:canvasY 348.0 ;
    ingen:enabled true ;
    ingen:polyphonic false ;
    lv2:microVersion 0 ;
    lv2:minorVersion 1 ;
    mod:builderVersion 1 ;
    mod:releaseNumber 3 ;
    lv2:port <Granulator/in0> ,
             <Granulator/in1> ,
             <Granulator/out0> ,
             <Granulator/out1> ,
             <Granulator/delay_length> ,
             <Granulator/grain_density> ,
             <Granulator/grain_length> ,
             <Granulator/mix> ,
             <Granulator/:bypass> ;
    lv2:prototype <http://faust-lv2.googlecode.com/Granulator> ;
    pedal:preset <> ;
    a ingen:Block .

<Granulator/in0>
    a lv2:AudioPort ,
        lv2:InputPort .

<Granulator/in1>
    a lv2:AudioPort ,
        lv2:InputPort .

<Granulator/in0>
    a lv2:AudioPort ,
        lv2:OutputPort .

<Granulator/in1>
    a lv2:AudioPort ,
        lv2:OutputPort .

<Granulator/grain_length>
    ingen:value 0.100000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Granulator/mix>
    ingen:value 0.500000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Granulator/grain_density>
    ingen:value 2.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Granulator/delay_length>
    ingen:value 10.000000 ;
    a lv2:ControlPort ,
        lv2:InputPort .

<Granulator/:bypass>
    ingen:value 0 ;
    a lv2:ControlPort ,
        lv2:InputPort .

